name: dockercompose17407843173652454045
services:
  discount-manager-mongodb:
    container_name: discount-manager-mongodb
    environment:
      MONGO_INITDB_ROOT_PASSWORD: rootpassword
      MONGO_INITDB_ROOT_USERNAME: root
    image: mongo:latest
    networks:
      discountmanager-network: null
    ports:
      - mode: ingress
        target: 27017
        published: "27019"
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\stmih\source\repos\DiscountManager\mongo-volume
        target: /data/discount-manager-mongodb
        bind:
          create_host_path: true
  discountmanager-client:
    build:
      context: C:\Users\stmih\source\repos\DiscountManager
      dockerfile: DiscountManager.Client/Dockerfile
    container_name: discountmanager-client
    networks:
      discountmanager-network: null
    stdin_open: true
    tty: true
  discountmanager-server:
    build:
      context: C:\Users\stmih\source\repos\DiscountManager
      dockerfile: DiscountManager.App/Dockerfile
    container_name: discount-manager-server
    depends_on:
      discount-manager-mongodb:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_HTTP_PORTS: "8080"
      ASPNETCORE_HTTPS_PORTS: "8081"
    image: discountmanagerapp
    networks:
      discountmanager-network: null
    ports:
      - mode: ingress
        target: 8080
        protocol: tcp
      - mode: ingress
        target: 8081
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\stmih\AppData\Roaming/Microsoft/UserSecrets
        target: /home/app/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\stmih\AppData\Roaming/ASP.NET/Https
        target: /home/app/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
networks:
  discountmanager-network:
    name: dockercompose17407843173652454045_discountmanager-network
    driver: bridge